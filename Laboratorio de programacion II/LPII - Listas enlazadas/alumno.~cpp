#include "alumno.h"
#include <string>

using namespace std;

alumno::alumno(string n, string a, int c)
{
        nombre=n;
        apellido=a;
        codigo=c;
}


void alumno::setNombre(string n)
{
        nombre=n;
}

void alumno::setApellido(string a)
{
        apellido=a;
}

void alumno::setCodigo(int c)
{
        codigo=c;
}


string alumno::getNombre()
{
        return nombre;
}


string alumno::getApellido()
{
        return apellido;
}


int alumno::getCodigo()
{
        return codigo;
}
